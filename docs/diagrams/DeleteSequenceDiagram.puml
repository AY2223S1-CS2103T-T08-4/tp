@startuml
!include style.puml

box Logic LOGIC_COLOR_T1
participant ":LogicManager" as LogicManager LOGIC_COLOR
participant ":SETAParser" as SETAParser LOGIC_COLOR
participant ":DeleteStuCommandParser" as DeleteCommandParser LOGIC_COLOR
participant "d:DeleteStuCommand" as DeleteCommand LOGIC_COLOR
participant ":CommandResult" as CommandResult LOGIC_COLOR
end box

box Model MODEL_COLOR_T1
participant ":Model" as Model MODEL_COLOR
end box

[-> LogicManager : execute("deletestu 1")
activate LogicManager

LogicManager -> SETAParser : parseCommand("deletestu 1")
activate SETAParser

create DeleteCommandParser
SETAParser -> DeleteCommandParser
activate DeleteCommandParser

DeleteCommandParser --> SETAParser
deactivate DeleteCommandParser

SETAParser -> DeleteCommandParser : parse("1")
activate DeleteCommandParser

create DeleteCommand
DeleteCommandParser -> DeleteCommand
activate DeleteCommand

DeleteCommand --> DeleteCommandParser : d
deactivate DeleteCommand

DeleteCommandParser --> SETAParser : d
deactivate DeleteCommandParser
'Hidden arrow to position the destroy marker below the end of the activation bar.
DeleteCommandParser -[hidden]-> SETAParser
destroy DeleteCommandParser

SETAParser --> LogicManager : d
deactivate SETAParser

LogicManager -> DeleteCommand : execute()
activate DeleteCommand

DeleteCommand -> Model : deleteStudent(1)
activate Model

Model --> DeleteCommand
deactivate Model

create CommandResult
DeleteCommand -> CommandResult
activate CommandResult

CommandResult --> DeleteCommand
deactivate CommandResult

DeleteCommand --> LogicManager : result
deactivate DeleteCommand

[<--LogicManager
deactivate LogicManager
@enduml
